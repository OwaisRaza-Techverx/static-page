<h1>Listing USERS</h1>
<%= will_paginate %> 
<table>
  <tr>
    <th>id</th>
    <th>NAME</th>
    <th>Email</th>
  </tr>

  <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><%= u.name %></td>
      <td><%= u.email %></td>     
      <td><%= link_to 'Show', user_path(u) %></td>
      <td><%= link_to 'Edit', edit_user_path(u) %></td>
      <td><%= link_to 'Destroy', user_path(u),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>


  
</table>
<%= will_paginate %>